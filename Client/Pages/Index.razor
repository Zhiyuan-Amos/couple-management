@page "/"

<div style="padding-bottom: 56px;" class="mh-100 overflow-auto">
    @foreach (var image in FavouriteImages)
    {
        <img class="w-100 h-auto"
             src="data:image/bmp;base64, @(Convert.ToBase64String(image.Data))"
             @onclick="@(() => EditImage(image))"/>
    }
</div>
<BottomBar/>
