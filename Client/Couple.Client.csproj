    <_ContentIncludedByDefault Remove="Pages\Calendar\Calendar.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\AnimatedCategoryTreeView.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\CalendarTopBar.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\CategoryTreeViewContent.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\CategoryTreeViewTopBar.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\CreateUpdateForm.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\Components\CreateUpdateTopBar.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\CreateEvent.razor" />
    <_ContentIncludedByDefault Remove="Pages\Calendar\UpdateEvent.razor" />
  </ItemGroup>
